<?php
$var1 = 4;
$var2 = 8;

if ( $var1<10 && $var1>5){
    echo "<li>" . "het getal is tussen 10 en 5!" . "</li>";
}
else {
    echo "<li>" . "het getal is groter dan 10 of kleiner dan 5!" . "</li>"
}

if ( $var2<10 | | $var2>5){
    echo "<li>" . "het getal is tussen 10 en 5!" . "</li>";
}
else {
    echo "<li>" . "het getal is groter dan 10 of kleiner dan 5!" . "</li>"
}

if ( $var1<10 xor $var1>5){
    echo "<li>" . "het getal is tussen 10 en 5!" . "</li>";
}
else {
    echo "<li>" . "het getal is groter dan 10 of kleiner dan 5!" . "</li>"
}

if ( $var2<10 ! $var2>5){
    echo "<li>" . "het getal is tussen 10 en 5!" . "</li>";
}
else {
    echo "<li>" . "het getal is groter dan 10 of kleiner dan 5!" . "</li>"
